[run]
# source = multiprocess
include =
    */multiprocess/*
#   */_multiprocess/*
omit = 
    */examples/*
    */multiprocess/tests/*
# timid = true
# parallel = true
# concurrency = multiprocessing # thread
# data_file = $TRAVIS_BUILD_DIR/.coverage
# data_file = ./.coverage
debug = trace

[paths]
source =
    py3.6
    */site-packages/multiprocess-*py3.6*

[report]
include =
    */multiprocess/*
#   */_multiprocess/*
exclude_lines =
    pragma: no cover
    if __name__ == .__main__.:
# show_missing = true
ignore_errors = true
